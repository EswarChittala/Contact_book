{
  "name": "contact-book-fullstack",
  "version": "1.0.0",
  "description": "Full-stack Contact Book with modern UI",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm start",
    "dev:frontend": "cd frontend-react && npm run dev",
    "build": "cd frontend-react && npm run build",
    "start": "cd backend && npm start"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
